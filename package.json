{
  "name": "frontend-angular-signals",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:browser": "vitest run --config vitest.config.browser.ts",
    "test:visual": "playwright test",
    "test:visual:ui": "playwright test --ui",
    "test:visual:report": "playwright show-report",
    "test:visual:grep": "playwright test --grep",
    "test:visual:list": "playwright test --list",
    "test:visual:desktop": "playwright test --grep \"desktop\"",
    "test:visual:mobile": "playwright test --grep \"mobile\"",
    "test:visual:tablet": "playwright test --grep \"tablet\"",
    "test:visual:list-only": "playwright test --grep \"list page\"",
    "test:visual:detail-only": "playwright test --grep \"detail page\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@angular/common": "^21.0.7",
    "@angular/compiler": "^21.0.7",
    "@angular/core": "^21.0.7",
    "@angular/forms": "^21.0.7",
    "@angular/platform-browser": "^21.0.7",
    "@angular/router": "^21.0.7",
    "@taiga-ui/addon-mobile": "^4.66.0",
    "@taiga-ui/addon-table": "^4.66.0",
    "@taiga-ui/cdk": "^4.66.0",
    "@taiga-ui/core": "^4.66.0",
    "@taiga-ui/event-plugins": "^4.7.0",
    "@taiga-ui/icons": "^4.66.0",
    "@taiga-ui/kit": "^4.66.0",
    "@taiga-ui/layout": "^4.66.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.1.0",
    "@angular-eslint/eslint-plugin": "^21.1.0",
    "@angular-eslint/eslint-plugin-template": "^21.1.0",
    "@angular-eslint/schematics": "^21.1.0",
    "@angular-eslint/template-parser": "^21.1.0",
    "@angular-eslint/utils": "^21.1.0",
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.7",
    "@angular/language-service": "^21.0.6",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "jsdom": "^27.4.0",
    "less": "^4.2.0",
    "or": "^0.2.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.16"
  }
}
